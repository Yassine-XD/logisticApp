                  "    pm.expect(summary.smallTires).to.eql(50);",
                  "    pm.expect(summary.mediumTires).to.eql(20);",
                  "    ",
                  "    console.log(\"‚úÖ Stop completed:\", {",
                  "        planned: summary.plannedKg + \" kg\",",
                  "        actual: summary.actualKg + \" kg\",",
                  "        small: summary.smallTires + \" tires\",",
                  "        medium: summary.mediumTires + \" tires\",",
                  "        difference: summary.difference + \" kg\"",
                  "    });",
                  "});"
