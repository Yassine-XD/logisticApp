    {
      "name": "2. Tour Workflow",
      "item": [
        {
          "name": "Request Tour",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status is 200 or 201\", function () {",
                  "    pm.expect([200, 201]).to.include(pm.response.code);",
                  "});",
                  "",
                  "var jsonData = pm.response.json();",
                  "",
                  "if (jsonData.tour) {",
                  "    pm.test(\"Tour created or reused\", function () {",
                  "        pm.expect(jsonData.tour._id).to.exist;",
                  "        pm.expect(jsonData.tour.stops).to.be.an('array');",
                  "        pm.expect(jsonData.tour.capacityKg).to.eql(3200);",
                  "        ",
                  "        // Save tour ID and first stop ID",
                  "        pm.collectionVariables.set(\"tourId\", jsonData.tour._id);",
                  "        ",
                  "        if (jsonData.tour.stops.length > 0) {",
                  "            pm.collectionVariables.set(\"stopId\", jsonData.tour.stops[0]._id);",
                  "            ",
                  "            console.log(\"‚úÖ Tour created:\", {",
                  "                id: jsonData.tour._id,",
                  "                stops: jsonData.tour.stops.length,",
                  "                distance: jsonData.tour.totalDistanceKm + \" km\",",
                  "                capacity: jsonData.tour.remainingCapacityKg + \" kg remaining\"",
                  "            });",
                  "            ",
                  "            console.log(\"First stop:\", {",
                  "                garage: jsonData.tour.stops[0].garageName,",
                  "                kg: jsonData.tour.stops[0].plannedKg,",
                  "                priority: jsonData.tour.stops[0].priority",
                  "            });",
                  "        }",
                  "    });",
                  "    ",
                  "    pm.test(\"Tour has stops\", function () {",
                  "        pm.expect(jsonData.tour.stops.length).to.be.above(0);",
                  "    });",
                  "    ",
                  "    pm.test(\"Stops have geo coordinates\", function () {",
                  "        jsonData.tour.stops.forEach(function(stop) {",
                  "            pm.expect(stop.geo).to.exist;",
                  "            pm.expect(stop.geo.lat).to.exist;",
                  "            pm.expect(stop.geo.lng).to.exist;",
                  "        });",
                  "    });",
                  "} else {",
                  "    console.log(\"‚ö†Ô∏è No tour created:\", jsonData.info);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
